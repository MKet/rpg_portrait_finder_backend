{
  "Routes": [
    {
      "DownstreamPathTemplate": "/todos/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/test/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ]
    },
    {
      "DownstreamPathTemplate": "/register",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authenticationservice",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/register",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authenticationservice",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/login",
      "UpstreamHttpMethod": [ "POST" ]
    }

  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7280"
  }
}
